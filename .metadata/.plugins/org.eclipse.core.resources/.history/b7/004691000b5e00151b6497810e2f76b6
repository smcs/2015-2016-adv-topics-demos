package smcs.battis.stringpatterns;

public class StringPatterns {

	/**
	 * Prints a line of n stars
	 * @param n The number of stars to print
	 */
	public static void lineOfStars(int n) {
		for (int i = 0; i < n; i++) {
			System.out.println("* ");
		}
	}
	
	public static void main(String[] args) {
		lineOfStars(10);
	}

}
